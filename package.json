{
  "name": "skryba",
  "version": "1.1.1",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "NX_APP_VERSION=$(node -p 'require(\"./package.json\").version') nx build",
    "build:api": "nx build api",
    "build:api-docker": "docker buildx build -f docker/api/Dockerfile -t skryba-api:latest --progress=plain --platform linux/amd64 .",
    "build:api-docker:native": "docker buildx build . -f docker/api/Dockerfile -t skryba-api:native --progress=plain",
    "heroku-postbuild": "nx build api --prod",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint && nx lint api",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "ts-node-dev": "TS_NODE_PROJECT=tsconfig.node.json ts-node-dev -r tsconfig-paths/register",
    "release": "semantic-release",
    "release:dry": "semantic-release -d",
    "update-workspace-variables": "ts-node -r tsconfig-paths/register tools/terraform/updateWorkspaceVariables.ts"
  },
  "private": true,
  "devDependencies": {
    "@babel/core": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "7.9.0",
    "@nrwl/cli": "11.5.1",
    "@nrwl/cypress": "11.5.1",
    "@nrwl/eslint-plugin-nx": "11.5.1",
    "@nrwl/express": "11.5.1",
    "@nrwl/jest": "11.5.1",
    "@nrwl/linter": "11.5.1",
    "@nrwl/node": "11.5.1",
    "@nrwl/nx-cloud": "latest",
    "@nrwl/react": "11.5.1",
    "@nrwl/tao": "11.5.1",
    "@nrwl/web": "11.5.1",
    "@nrwl/workspace": "11.5.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/exec": "^5.0.0",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.2.0",
    "@semantic-release/npm": "^7.0.5",
    "@testing-library/react": "11.1.2",
    "@types/classnames": "^2.2.11",
    "@types/downloadjs": "^1.4.2",
    "@types/express": "4.17.0",
    "@types/jest": "26.0.8",
    "@types/node": "12.12.38",
    "@types/node-fetch": "^2.5.8",
    "@types/react": "16.9.56",
    "@types/react-dom": "16.9.9",
    "@types/react-router-dom": "^5.1.7",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "babel-jest": "26.2.2",
    "chalk": "^4.1.0",
    "cypress": "^6.0.1",
    "cypress-file-upload": "^5.0.3",
    "dotenv": "6.2.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.21.5",
    "eslint-plugin-react-hooks": "4.2.0",
    "glob": "^7.1.4",
    "jest": "26.2.2",
    "prettier": "2.2.1",
    "ts-jest": "26.4.0",
    "ts-node": "~9.1.1",
    "ts-node-builder": "^2.2.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.3",
    "yargs": "^15.4.1"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.8",
    "@chakra-ui/react": "^1.4.2",
    "@chakra-ui/system": "^1.6.0",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "awilix": "^4.3.3",
    "classnames": "^2.2.6",
    "core-js": "^3.6.5",
    "document-register-element": "1.13.1",
    "downloadjs": "^1.4.7",
    "escape-string-regexp": "^4.0.0",
    "fastify": "^3.14.0",
    "fastify-cors": "^5.2.0",
    "framer-motion": "3.10.6",
    "joiful": "^3.0.0",
    "node-fetch": "^2.6.1",
    "nx": "11.5.2",
    "puppeteer": "8.0.0",
    "react": "17.0.1",
    "react-dnd": "^14.0.2",
    "react-dnd-html5-backend": "^14.0.0",
    "react-dom": "17.0.1",
    "react-hook-form": "^6.15.5",
    "react-query": "^3.13.0",
    "react-router-dom": "^5.2.0",
    "react-string-replace": "^0.4.4",
    "react-use": "^17.2.1",
    "reflect-metadata": "^0.1.13",
    "remeda": "^0.0.27",
    "ts-key-enum": "^2.0.7",
    "tslib": "^2.0.0",
    "uuid": "^3.4.0",
    "zustand": "^3.3.3"
  },
  "release": {
    "branches": [
      "+([0-9])?(.{+([0-9]),x}).x",
      "master",
      "next",
      {
        "name": "alpha",
        "prerelease": true
      },
      {
        "name": "beta",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "docs/CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs/CHANGELOG.md",
            "package.json",
            "package-lock.json"
          ]
        }
      ],
      [
        "@semantic-release/github"
      ]
    ]
  }
}
