{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/react" },
  "defaultProject": "pable",
  "generators": {
    "@nrwl/react": {
      "application": { "style": "css", "linter": "eslint", "babel": true },
      "component": { "style": "css" },
      "library": { "style": "css", "linter": "eslint" }
    }
  },
  "projects": {
    "pable": {
      "root": "apps/pable",
      "sourceRoot": "apps/pable/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/pable",
            "index": "apps/pable/src/index.html",
            "main": "apps/pable/src/main.tsx",
            "polyfills": "apps/pable/src/polyfills.ts",
            "tsConfig": "apps/pable/tsconfig.app.json",
            "assets": ["apps/pable/src/favicon.ico", "apps/pable/src/assets"],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/pable/src/environments/environment.ts",
                  "with": "apps/pable/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": true,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "pable:build",
            "proxyConfig": "apps/pable/proxy.conf.json"
          },
          "configurations": {
            "production": { "buildTarget": "pable:build:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/pable/**/*.{ts,tsx,js,jsx}"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/pable"],
          "options": {
            "jestConfig": "apps/pable/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "pable-e2e": {
      "root": "apps/pable-e2e",
      "sourceRoot": "apps/pable-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/pable-e2e/cypress.json",
            "tsConfig": "apps/pable-e2e/tsconfig.e2e.json",
            "devServerTarget": "pable:serve"
          },
          "configurations": {
            "production": { "devServerTarget": "pable:serve:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/pable-e2e/**/*.{js,ts}"] }
        }
      }
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": ["apps/api/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": { "buildTarget": "api:build" }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["apps/api/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/api"],
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "api-interfaces": {
      "root": "libs/api-interfaces",
      "sourceRoot": "libs/api-interfaces/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/api-interfaces/**/*.ts"] }
        }
      }
    },
    "domain": {
      "root": "libs/domain",
      "sourceRoot": "libs/domain/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/domain/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/domain"],
          "options": {
            "jestConfig": "libs/domain/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "domain-types": {
      "root": "libs/domain-types",
      "sourceRoot": "libs/domain-types/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/domain-types/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/domain-types"],
          "options": {
            "jestConfig": "libs/domain-types/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "server-types": {
      "root": "libs/server-types",
      "sourceRoot": "libs/server-types/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": { "lintFilePatterns": ["libs/server-types/**/*.ts"] }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/server-types"],
          "options": {
            "jestConfig": "libs/server-types/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
